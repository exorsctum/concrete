mod Example {
    fn main() -> i64 {
        return sum_to(4);
    }

    fn sum_to(limit: i64) -> i64 {
        let mut result: i64 = 0;

        let mut n: i64 = 1;
        for {
            if n > 10 {
                // there is no break yet
                return result;
            }
            result = result + n;
            n = n + 1;
        }
    }
}
